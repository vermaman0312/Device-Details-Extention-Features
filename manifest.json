{
  "name": "Extension 1",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "A simple extension to block websites",
  "action": {
    "default_popup": "./popup.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["activeTab", "tabs", "storage"]
}
